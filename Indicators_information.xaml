<Window x:Class="School_Project.Indicators_information"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:School_Project"
        mc:Ignorable="d"
        Title="Indicators_information" MaxHeight="700" MaxWidth="450" MinHeight="700" MinWidth="450" Background="#0097a9">
    <Grid>
        <StackPanel HorizontalAlignment="Center" Margin="30, 0, 30, 0">
            <TextBlock x:Name="Name" FontSize="20" TextWrapping="Wrap" TextAlignment="Center" FontWeight="DemiBold" FontFamily="Verdana" Margin="0, 40, 0, 0" Foreground="White"></TextBlock>
            <TextBlock x:Name="text" FontSize="17" TextWrapping="Wrap" TextAlignment="Justify" Padding="10" Foreground="White"></TextBlock>
            <TextBlock x:Name="Up" FontSize="17" TextWrapping="Wrap" TextAlignment="Center" Margin="0, 20" FontWeight="Medium" Foreground="White"></TextBlock>
            <TextBlock x:Name="Up1" FontSize="17" TextWrapping="Wrap" TextAlignment="Center" Foreground="White"></TextBlock>
            <TextBlock x:Name="Up2" FontSize="17" TextWrapping="Wrap" TextAlignment="Center" Foreground="White"></TextBlock>
            <TextBlock x:Name="Up3" FontSize="17" TextWrapping="Wrap" TextAlignment="Center" Foreground="White"></TextBlock>
            <TextBlock x:Name="Up4" FontSize="17" TextWrapping="Wrap" TextAlignment="Center" Foreground="White"></TextBlock>
            <TextBlock x:Name="Up5" FontSize="17" TextWrapping="Wrap" TextAlignment="Center" Foreground="White"></TextBlock>
            <TextBlock x:Name="Up6" FontSize="17" TextWrapping="Wrap" TextAlignment="Center" Foreground="White"></TextBlock>
            <TextBlock x:Name="Dow" FontSize="17" TextWrapping="Wrap" TextAlignment="Center" Margin="0, 20" FontWeight="Medium" Foreground="White"></TextBlock>
            <TextBlock x:Name="Down1" FontSize="17" TextWrapping="Wrap" TextAlignment="Center" Foreground="White"></TextBlock>
            <TextBlock x:Name="Down2" FontSize="17" TextWrapping="Wrap" TextAlignment="Center" Foreground="White"></TextBlock>
            <TextBlock x:Name="Down3" FontSize="17" TextWrapping="Wrap" TextAlignment="Center" Foreground="White"></TextBlock>
        </StackPanel>
       
    </Grid>
</Window>
